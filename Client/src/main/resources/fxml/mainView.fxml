<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="800.0" prefWidth="600.0" style="-fx-background-color: #242424;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="messageField" layoutX="86.0" layoutY="698.0" prefHeight="39.0" prefWidth="429.0" promptText="message" style="-fx-background-color: WHITE;" />
      <TextField fx:id="portField" alignment="TOP_LEFT" layoutX="509.0" layoutY="34.0" prefHeight="25.0" prefWidth="44.0" promptText="port" style="-fx-background-color: #404040; -fx-text-fill: WHITE;" />
      <TextField fx:id="hostField" alignment="TOP_LEFT" layoutX="390.0" layoutY="34.0" prefHeight="25.0" prefWidth="112.0" promptText="host" style="-fx-background-color: #404040; -fx-text-fill: WHITE;" />
      <Button layoutX="478.0" layoutY="76.0" mnemonicParsing="false" onMousePressed="#onConnectButtonClicked" prefHeight="25.0" prefWidth="75.0" style="-fx-background-color: #404040;" text="Connect" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="11.0" />
         </font>
      </Button>
      <TextArea fx:id="chat" editable="false" layoutX="86.0" layoutY="127.0" prefHeight="505.0" prefWidth="429.0" promptText="not connected" style="-fx-prompt-text-fill: WHITE; -fx-control-inner-background: #404040;">
         <font>
            <Font name="Arial Black" size="15.0" />
         </font>
      </TextArea>
      <Button layoutX="390.0" layoutY="76.0" mnemonicParsing="false" onMousePressed="#onDisconnectButtonClicked" prefHeight="25.0" prefWidth="75.0" style="-fx-background-color: #404040;" text="Disconnect" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="9.0" />
         </font>
      </Button>
      <Button layoutX="527.0" layoutY="698.0" mnemonicParsing="false" onMousePressed="#onEnterButtonClicked" prefHeight="39.0" prefWidth="59.0" text="Enter" />
      <TextField fx:id="userNameField" layoutX="86.0" layoutY="34.0" promptText="username" style="-fx-background-color: #404040; -fx-text-fill: WHITE;" />
   </children>
</AnchorPane>
